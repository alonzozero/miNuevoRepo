<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.genesis" name="Genesis" version="3.2.1" provider-name="lambda">
	<requires>
		<import addon="xbmc.python" version="2.1.0"/>
		<import addon="script.module.urlresolver" version="1.0.0"/>
		<import addon="script.module.metahandler" optional="true"/>
		<import addon="script.module.simplejson" optional="true"/>
		<import addon="script.module.parsedom" optional="true"/>
		<import addon="plugin.video.youtube" optional="true"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.service" library="service.py" start="startup">
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">The origins of streaming</summary>
		<description lang="en">Visit TVADDONS.ag for support</description>
		<disclaimer lang="en">The author does not host or distribute any of the content displayed by this addon. The author does not have any affiliation with the content provider.</disclaimer>
		<forum>http://forums.tvaddons.ag/forums/148-lambda-s-xbmc-addons</forum>
		<source>https://offshoregit.com/lambda81/</source>
		<website>https://offshoregit.com/lambda81/</website>
		<platform>all</platform>
	</extension>
</addon>


<addon id="repository.kodi-mexico" name="Repositorio Oficial Cybernet" version="1.1.0" provider-name="alonzozero">
	<extension point="xbmc.addon.repository" name="Repositorio Oficial Cybernet">
		<info compressed="false">https://raw.githubusercontent.com/alonzozero/miNuevoRepo/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/alonzozero/miNuevoRepo/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/alonzozero/miNuevoRepo/master</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Repositorio Oficial Cybernet</summary>
   		<description lang="en">Instala los Addons Cybernet.</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
